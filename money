let money = 3752;
const ark = [1000, 500, 200, 100, 50, 20, 10, 5, 2, 1];
const arr = [];
let i = 0;
    let value = money;
    while(value > 0){
        if(value >= ark[i]){
            value = value - ark[i];
            arr.push(ark[i])
        }
        else{
           i++;

        }

    }
console.log(arr);
